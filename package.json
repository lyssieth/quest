{
	"name": "quest",
	"version": "0.0.1",
	"license": "Apache-2.0",
	"private": true,
	"scripts": {
		"dev": "pnpm sync && vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"check": "pnpm sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "pnpm sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "pnpm sync && husky install",
		"check-lint": "pnpm sync && pnpm format && pnpm lint && pnpm check"
	},
	"devDependencies": {
		"@fontsource/montserrat": "^5.0.3",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.20.2",
		"@typescript-eslint/eslint-plugin": "^5.59.11",
		"@typescript-eslint/parser": "^5.59.11",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.30.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.63.3",
		"svelte": "^3.59.1",
		"svelte-adapter-deno": "^0.9.0",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.5.3",
		"typescript": "^5.1.3",
		"vite": "^4.3.9"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/inter": "^5.0.3"
	}
}
